{
    "name": "letoceiling-coder/deploy",
    "description": "Automated deployment package for Laravel projects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Letoceiling Coder",
            "email": "dev@letocewh.beget.tech"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "LetoceilingCoder\\Deploy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LetoceilingCoder\\Deploy\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LetoceilingCoder\\Deploy\\DeployServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

