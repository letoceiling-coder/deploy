{
    "name": "letoceiling-coder/deploy",
    "description": "Automated deployment package for Laravel projects with Git and HTTP API integration",
    "type": "library",
    "keywords": [
        "laravel",
        "deploy",
        "deployment",
        "git",
        "automation",
        "ci-cd",
        "laravel-package"
    ],
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Letoceiling Coder",
            "email": "dev@letocewh.beget.tech"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "LetoceilingCoder\\Deploy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LetoceilingCoder\\Deploy\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LetoceilingCoder\\Deploy\\DeployServiceProvider"
            ],
            "aliases": {}
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "homepage": "https://github.com/letoceiling-coder/deploy",
    "support": {
        "issues": "https://github.com/letoceiling-coder/deploy/issues",
        "source": "https://github.com/letoceiling-coder/deploy"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/letoceiling-coder/deploy.git"
        }
    ]
}

